Функция ТаблицаИсключений(ИнформационнаяБаза) Экспорт
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	ИсключаемыеПользователи.Пользователь КАК Пользователь
	|ИЗ
	|	РегистрСведений.ИсключаемыеПользователи КАК ИсключаемыеПользователи
	|ГДЕ
	|	ИсключаемыеПользователи.ИнформационнаяБаза = &ИнформационнаяБаза";
	
	Запрос.УстановитьПараметр("ИнформационнаяБаза", ИнформационнаяБаза);
	Возврат Запрос.Выполнить().Выгрузить();
КонецФункции	

Функция ТаблицаРаботающих(ИнформационнаяБаза)
	ОшибкаHTTP = "";
	//ТблОбщ - общая таблиза значений для работающих - нужно когда мы берем работающих из двух и более ЗУП (например МТС и НСТ);
	ТблОбщ = Неопределено;
	
	Для Каждого ХХХ Из ИнформационнаяБаза.БазыЗУП Цикл
		СткЗапроса = ОбщегоНазначения.ПолучитьСткЗапроса();
		ЗаполнитьЗначенияСвойств(СткЗапроса,ХХХ.БазаЗУП);
		СткЗапроса.ИНН = ХХХ.БазаЗУП.Организация.ИНН;
		СткЗапроса.ИмяМетода = "GETRABSOTR";
		Тбл = ОбщегоНазначения.ПолучитьКадрТбл(СткЗапроса,ОшибкаHTTP);
		
		//ОШИБКА - в регистр - невозможно получить таблицу работающих 
		Если Тбл = 1 Тогда
			//РезультатЗаписи = ОбщегоНазначения.ЗаписьВРегистрОшибок(ТекущаяДата(),ИнформационнаяБаза,ХХХ.БазаЗУП.Наименование,"Не получена таблица работающих. " + ОшибкаHTTP);
			//Если РезультатЗаписи = 1 Тогда //по какой-то причине в регистр сведений не удалось записать (очень маловероятно, но все же). Пишем в системный журнал
			//	 ЗаписьЖурналаРегистрации("Ошибка записи в РС ""Ошибки обработки списка пользователей"".",УровеньЖурналаРегистрации.Ошибка,РегистрыСведений.ОшибкиОбработкиСпискаПользователей,,ХХХ.БазаЗУП,);
			//КонецЕсли;	
			Продолжить;
		КонецЕсли;
		
		Если ТблОбщ = Неопределено Тогда //Ни разу еще в эту таблицу данные не загружались
			ТблОбщ = Тбл;
		Иначе
			Для Каждого стр Из Тбл Цикл
				нСтр = ТблОбщ.Добавить();
				ЗаполнитьЗначенияСвойств(нСтр,стр);
			КонецЦикла;	
		КонецЕсли;	
		
	КонецЦикла;
	
	Если ТблОбщ = Неопределено Тогда
 		Возврат 1;
	Иначе
		ТблОбщ.Свернуть("ФИО");
		Возврат ТблОбщ;
	КонецЕсли;	
КонецФункции	

Функция ОтправитьПисьмо(ПользователиИБ,ИнформационнаяБаза)
	Перем ОшибкаHTTP;
	Сотрудники = "";
	
	МассивПолучателей = ОбщегоНазначения.РазложитьСтрокуВМассивПодстрок(ИнформационнаяБаза.EMail,";");
	Если МассивПолучателей.Количество() = 0 Тогда
		ОписаниеОшибки = "Отсутствует перечень адресатов для информирования в карточке базы.";
		ЗаписьВРегистрОтчета(ИнформационнаяБаза,ОписаниеОшибки);		
		Возврат 1;
	КонецЕсли;	
	
	Для Каждого ХХХ Из ПользователиИБ Цикл
		Сотрудники = Сотрудники + ХХХ.ПользовательИБ + "<br>";
	КонецЦикла;	
	
	ТемаПисьма = "Блокируемые / удаляемые пользователи за " + Строка(Формат(ТекущаяДата(),"ДЛФ=ДД"));
	
	ТелоПисьма = ПолучитьТелоПисьма();
	ТелоПисьма = СтрЗаменить(ТелоПисьма,"==ДАТА==",Строка(Формат(ТекущаяДата(),"ДЛФ=ДД")));
	ТелоПисьма = СтрЗаменить(ТелоПисьма,"==ЗАГОЛОВОК==","Информационная база: " + ИнформационнаяБаза);
	ТелоПисьма = СтрЗаменить(ТелоПисьма,"==ПОДЗАГЛОВОК==","В связи с увольнением/потерей актуальности с сегодняшнего дня будут заблокированы/удалены следующие учетные записи пользователей");
	ТелоПисьма = СтрЗаменить(ТелоПисьма,"==Сотрудники==",Сотрудники);
	
	Результат = ОбщегоНазначения.ПереслатьПочтуАЗУР(МассивПолучателей,,,ТемаПисьма,ТелоПисьма,,,,ОшибкаHTTP);
	
	//!!!ОШИБКА в регистр - не смогли отправить письмо
	Если Результат = 1 Тогда
		ОписаниеОшибки = "Не удалось отправить эл. письмо.";
		ЗаписьВРегистрОтчета(ИнформационнаяБаза,ОписаниеОшибки);		
	КонецЕсли;	
	
	Возврат Результат;
КонецФункции

Процедура ВыполнитьОбработкуИБ(ИнформационнаяБаза) Экспорт
	Перем ОшибкаHTTP;
	ОписаниеОшибки = "";
	
	//Получим из регистра уже исключенных пользователей
	ТблИскл = ТаблицаИсключений(ИнформационнаяБаза);
	
	//Получим таблицу работающих сотрудников
	Тбл = ТаблицаРаботающих(ИнформационнаяБаза);
	
	//!!!ОШИБКА в регистр - вообще не получисли список работающих из ЗУП
	Если Тбл = 1 Тогда
		ОписаниеОшибки = "ЗУП. Не получены данные о работающих. Обработка базы прервана и не выполнена.";
		ЗаписьВРегистрОтчета(ИнформационнаяБаза,ОписаниеОшибки);		
		Возврат;
	КонецЕсли;	
	
	//Получим массив пользователей информационной базы
	СткЗапроса = ОбщегоНазначения.ПолучитьСткЗапроса();
	ЗаполнитьЗначенияСвойств(СткЗапроса,ИнформационнаяБаза);
	СткЗапроса.ИмяМетода = "USERSLIST";
	Мас = ОбщегоНазначения.ВыполнитьЗапрос(СткЗапроса,ОшибкаHTTP);
	Если Мас = 1 Тогда
		ОписаниеОшибки = "Не получен массив пользователей. " + ОшибкаHTTP + " Обработка базы прервана и не выполнена.";
		ЗаписьВРегистрОтчета(ИнформационнаяБаза,ОписаниеОшибки);		
		Возврат;
	КонецЕсли;	
	
	//ПользователиИБ - ТЗ, которая содержит в себе перечень пользователей ИБ, которых нет ни в списке работающих, ни в списке исключаемых
	ПользователиИБ = Новый ТаблицаЗначений;
	ПользователиИБ.Колонки.Добавить("Имя",Новый ОписаниеТипов("Строка"));
	ПользователиИБ.Колонки.Добавить("ПолноеИмя",Новый ОписаниеТипов("Строка"));
	ПользователиИБ.Колонки.Добавить("ГУИД",Новый ОписаниеТипов("Строка"));
	ПользователиИБ.Колонки.Добавить("Удалить",Новый ОписаниеТипов("Булево"));
	ПользователиИБ.Колонки.Добавить("Восстановить",Новый ОписаниеТипов("Булево"));
	
	ТекДата = "(" + Строка(Формат(ТекущаяДата(),"ДЛФ=Д")) + ")";
	
	Для каждого ХХХ Из Мас Цикл
		
		//Определим сначала ислючаемых
		СтруктураПоиска = Новый Структура("Пользователь", ХХХ.ПолноеИмя); 
		МассивНайденныхСтрок = ТблИскл.НайтиСтроки(СтруктураПоиска); 	
		фИсключаемый = (МассивНайденныхСтрок.Количество() <> 0);
		Если фИсключаемый Тогда
			Продолжить;
		КонецЕсли;	
		
		//Поищем в работающих
		фВосстановить = Ложь;
		СтруктураПоиска = Новый Структура("ФИО", СокрЛП(ХХХ.ПолноеИмя)); 
		МассивНайденныхСтрок = ТБл.НайтиСтроки(СтруктураПоиска); 	
		фРаботник = (МассивНайденныхСтрок.Количество() <> 0);
		фЗаблокирован = (СтрНайти(ХХХ.Имя,"(") <> 0);
		Если фРаботник Тогда //нашли в списке работающих
			Если Не фЗаблокирован  Тогда
				Продолжить;
			Иначе //восстановим УЗ заблокированного пользователя
				фВосстановить = Истина;
			КонецЕсли;
		ИначеЕсли фЗаблокирован Тогда
			Продолжить;
		КонецЕсли;	
		
		нСтр = ПользователиИБ.Добавить();
		нСтр.ПолноеИмя = ХХХ.ПолноеИмя;
		нСтр.Имя = ХХХ.Имя;
		нСтр.ГУИД = ХХХ.УникальныйИдентификатор;
		нСтр.Удалить = (СтрНайти(ХХХ.Имя,ТекДата) <> 0);
		нСтр.Восстановить = фВосстановить;
		
	КонецЦикла;	
	
	Если ПользователиИБ.Количество() > 0 Тогда //Список для обработки не пустой
		//Проанализируем его: там могут быть строки трех типов - "К блокировке", "К удалению" и "К восстановлению"
		//Обработаем перечень пользователей
		ОшибкаHTTP = "";
		РезультатОбработки = ОтправитьНаОбработкуСписокПользователей(ПользователиИБ,ИнформационнаяБаза,ОшибкаHTTP);
		
		Если РезультатОбработки = 1 Тогда
			РезультатЗаписи = ОбщегоНазначения.ЗаписьВРегистрОшибок(ТекущаяДата(),ИнформационнаяБаза,);
			ОписаниеОшибки = "Обработка списка пользователей базы - " + ОшибкаHTTP;
			ЗаписьВРегистрОтчета(ИнформационнаяБаза,ОписаниеОшибки);		
			Возврат;
		КонецЕсли;	
		
	Иначе //Все пользователи ИБ пока работают - нет ни одного к блокировке	
		НаборЗаписей = РегистрыСведений.ОтчетОбОбработкеИБ.СоздатьНаборЗаписей();
		НаборЗаписей.Отбор.ДатаОбработки.Установить(ТекущаяДата()); 
		НоваяЗапись = НаборЗаписей.Добавить(); 
		НоваяЗапись.ДатаОбработки = ТекущаяДата();
		НоваяЗапись.ИнформационнаяБаза = ИнформационнаяБаза;
		НоваяЗапись.ПользовательИБ = ИнформационнаяБаза.Наименование;
		НоваяЗапись.фОбработан= "ИБ обработана. Пользователей к блокировке нет.";
		НаборЗаписей.Записать();
	КонецЕсли;	
	
	Если ТипЗнч(РезультатОбработки) = Тип("ТаблицаЗначений") И РезультатОбработки.Количество() > 0 Тогда 
		НаборЗаписей = РегистрыСведений.ОтчетОбОбработкеИБ.СоздатьНаборЗаписей();
		НаборЗаписей.Отбор.ДатаОбработки.Установить(ТекущаяДата()); 
		Для Каждого ХХХ Из РезультатОбработки Цикл
			НоваяЗапись = НаборЗаписей.Добавить(); 
			НоваяЗапись.ДатаОбработки = ТекущаяДата(); 
			НоваяЗапись.ИнформационнаяБаза = ИнформационнаяБаза; 
			НоваяЗапись.ПользовательИБ = ХХХ.ПользовательИБ; 
			НоваяЗапись.фОбработан = ХХХ.фОбработан; 
		КонецЦикла;	
		НаборЗаписей.Записать();
		
		//Отправим информационное письмо - сюда должны попасть только те УЗ, которые "Заблокирован"
		СтруктураПоиска = Новый Структура("фОбработан", "Заблокирован"); 
		МассивНайденныхСтрок = РезультатОбработки.НайтиСтроки(СтруктураПоиска); 	
		Если  МассивНайденныхСтрок.Количество() > 0 Тогда
			РезультатОтправки = ОтправитьПисьмо(МассивНайденныхСтрок,ИнформационнаяБаза);
			//Возврат;
		КонецЕсли;	
		
	Иначе //это случай, когда для обработки отправлен другой тип - НЕ таблица значений	
	КонецЕсли;	
	
КонецПроцедуры	

