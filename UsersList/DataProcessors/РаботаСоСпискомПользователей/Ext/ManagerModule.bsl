Функция ОтправитьНаОбработкуСписокПользователей(ПользователиИБ,ИнформационнаяБаза,ОшибкаHTTP = "")
	
	Соединение = Новый HTTPСоединение(
									ИнформационнаяБаза.АдресСервера, 	// сервер (хост)
									80, 								// порт, по умолчанию для http используется 80, для https 443
									ИнформационнаяБаза.Логин, 			// пользователь для доступа к серверу (если он есть) 
									ИнформационнаяБаза.Пароль,	 		// пароль для доступа к серверу (если он есть) 
									, 									// здесь указывается прокси, если он есть
									, 									// таймаут в секундах, 0 или пусто - не устанавливать
																		// защищенное соединение, если используется https
									);
	
	ТелоЗапроса = XMLСтрока(Новый ХранилищеЗначения(ПользователиИБ, Новый СжатиеДанных(-1)));
	Запрос = Новый HTTPЗапрос("/" + ИнформационнаяБаза.НаименованиеБазы + "/hs/" + ИнформационнаяБаза.АПИ + "/OBRABOTKAUSERSLIST");
	Запрос.Заголовки.Вставить("Content-Type","text/html; charset=utf-8");
	Запрос.УстановитьТелоИзСтроки(ТелоЗапроса);
	Попытка
		Ответ = Соединение.ОтправитьДляОбработки(Запрос);
		КодСостояния = Ответ.КодСостояния;
		Если КодСостояния = "200" или КодСостояния = 200 Тогда
			Попытка
				ТелоОтвета = Ответ.ПолучитьТелоКакСтроку();
				Тбл = XMLЗначение(Тип("ХранилищеЗначения"),ТелоОтвета).Получить();
				Возврат Тбл;
			Исключение
				ОшибкаHTTP = "Ошибка HTTP-запроса. Невозможно получить данные из тела ответа. " + Строка(ИнформацияОбОшибке().Описание);
				Возврат 1;
			КонецПопытки;
		Иначе
			ОшибкаHTTP = "Ошибка HTTP-запроса. Код состояния = " + КодСостояния;
			Возврат 1;
		КонецЕсли;
	Исключение
		ОшибкаHTTP = "Ошибка HTTP-запроса. Невозможно отправить запрос на адрес сервера. " + Строка(ИнформацияОбОшибке().Описание);
		Возврат 1;
	КонецПопытки;	
	
	Возврат 0;
КонецФункции

Процедура ЗаписьВРегистрОтчета(ИнформационнаяБаза,ОписаниеОшибки = "")
	НаборЗаписей = РегистрыСведений.ОтчетОбОбработкеИБ.СоздатьНаборЗаписей();
	НаборЗаписей.Отбор.ДатаОбработки.Установить(ТекущаяДата()); 
	НоваяЗапись = НаборЗаписей.Добавить(); 
	НоваяЗапись.ДатаОбработки = ТекущаяДата();
	НоваяЗапись.ИнформационнаяБаза = ИнформационнаяБаза;
	НоваяЗапись.ПользовательИБ = ИнформационнаяБаза.Наименование;
	НоваяЗапись.фОбработан= "ИБ не обработана.";
	НоваяЗапись.ОписаниеОшибки= ОписаниеОшибки;
	НаборЗаписей.Записать();
КонецПроцедуры
